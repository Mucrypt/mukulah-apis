{
  "name": "ecommerce-backend",
  "version": "1.0.0",
  "description": "High-performance e-commerce backend API with MySQL",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seeder": "node seeder.js",
    "setup": "npm run migrate && npm run seeder",
    "test:unit": "mocha tests/unit/**/*.test.js",
    "test:integration": "mocha tests/integration/**/*.test.js",
    "test": "npm run test:unit && npm run test:integration",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "husky install",
    "cleanup": "node cleanupCategoryIndexes.js",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:create": "sequelize migration:generate --name",
    "migrate:reset": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "migrate": "sequelize-cli db:migrate --config config/sequelizeConfig.js",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo:all"
  },
  "keywords": [
    "ecommerce",
    "nodejs",
    "mysql",
    "rest-api"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "@tanstack/react-query": "^5.74.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.6.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.14.0",
    "nodemailer": "^6.10.1",
    "redis": "^4.7.0",
    "sequelize": "^6.37.7",
    "slugify": "^1.6.6",
    "stripe": "^18.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4"
  }
}
